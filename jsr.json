{
  "name": "@apexlang/apex",
  "version": "0.2.6",
  "description": "Apex is a scaffolding, code generation, and task runner for all projects across all languages.",
  "keywords": ["apex", "idl", "codegen"],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/apexlang/apex"
  },
  "bugs": {
    "url": "https://www.github.com/apexlang/apex/issues"
  },
  "exports": {
    ".": "./mod.ts",
    "./mod.ts": "./mod.ts",
    "./config": "./src/config.ts",
    "./task": "./src/task.ts"
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "mod.ts",
      "deps/**/*.ts",
      "src"
    ],
    "exclude": ["src/tests"]
  }
}
